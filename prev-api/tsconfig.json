{
    "compilerOptions": {
      "target": "es2016",
      "module": "commonjs",
      "resolveJsonModule": true,
      "outDir": "./dist",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "plugins": [
        {
          "name": "@effect/language-service"
        }
      ]
    }
  }
  